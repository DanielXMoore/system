{
  "plugins": [
    "./build/jsdoc-coffee.js",
    "jsdoc-plugin-typescript"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./source/"
    ],
    "includePattern": ".+\\.coffee$|.+\\.ts",
    "excludePattern": "(/|\\\\)test(/|\\\\)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "typescript": {
    "moduleRoot": "source"
  }
}
